{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true,
        "jest": true        
    },
    "extends": [
        "eslint:recommended"        
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars": 2,
      "semicolon": [
        "error",
        "always"
      ],
    }
}
